{
    "version": 2,
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "trailingSlash": false,
    "cleanUrls": true,
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/api/(.*)",
        "status": 404
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }